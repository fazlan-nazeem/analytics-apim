<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<scenarioConfiguration  domain="APIMAnalytics" name="AbnormalResponseAndBackendTimeDetection"
					   scenario="AbnormalResponseAndBackendTimeDetection">
	<description>Detects abnormal backend time and response time</description>
	<parameterMap>
		<entry>
			<key>alertSuppressionPeriod</key>
			<value>10</value>
		</entry>
		<entry>
        	<key>cacheTimeout</key>
            <value>300</value>
        </entry>
		<entry>
			<key>responseTimeSeverity</key>
			<value>2</value>
		</entry>
		<entry>
			<key>backendTimeSeverity</key>
			<value>3</value>
		</entry>
	</parameterMap>
</scenarioConfiguration>
