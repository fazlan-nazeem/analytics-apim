<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<scenarioConfiguration domain="APIMAnalytics" name="UnusualIPAccessAlert" scenario="UnusualIPAccessTemplate">
	<description>Detecting accesses from new or rarely used hostnames</description>
	<parameterMap>
		<entry>
			<key>maxDaysBetweenAccess</key>
			<value>30</value>
		</entry>
		<entry>
			<key>severity</key>
			<value>2</value>
		</entry>
		<entry>
			<key>learningCount</key>
			<value>500</value>
		</entry>
	</parameterMap>
</scenarioConfiguration>
